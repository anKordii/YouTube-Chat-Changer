{
    "browser_action": {
       "default_icon": {
          "16": "icons/16.png",
          "32": "icons/32.png"
       },
       "default_popup": "popup.html"
    },
    "content_scripts": [ {
        "all_frames": true,
        "exclude_matches": [ "*://*.youtube.com/*.html", "*://*.youtube.com/*.html?*", "*://*.youtube.com/*.htm", "*://*.youtube.com/*.htm?*" ],
        "js": [ "run.js" ],
        "matches": [ "*://*.youtube.com/*" ]
    } ],
    "description": "Change default youtube chat to one of avilable on list.",
    "icons": {
       "128": "icons/128.png",
       "48": "icons/48.png"
    },
    "manifest_version": 2,
    "name": "YouTube Chat Changer",
    "permissions": [ "*://*.youtube.com/*" ],
    "version": "1.2"
 }